generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ShortUrl {
  id          Int      @id @default(autoincrement())
  slug        String   @unique
  originalUrl String
  createdAt   DateTime @default(now())
  clickCount  Int      @default(0)
}
